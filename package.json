{
  "name": "markdown-viewer-main",
  "version": "1.0.0",
  "description": "A powerful GitHub-style Markdown rendering tool",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node -e \"const fs=require('fs');const path=require('path');const dist='dist';if(!fs.existsSync(dist)){fs.mkdirSync(dist,{recursive:true});}['index.html','styles.css','script.js'].forEach(f=>fs.copyFileSync(f,path.join(dist,f)));const assets='assets';const distAssets=path.join(dist,assets);if(fs.existsSync(assets)){if(!fs.existsSync(distAssets)){fs.mkdirSync(distAssets,{recursive:true});}fs.readdirSync(assets).forEach(f=>fs.copyFileSync(path.join(assets,f),path.join(distAssets,f)));}\"",
    "preview": "vite preview",
    "tauri": "tauri",
    "desktop-internal": "tauri dev",
    "desktop": "run-desktop.bat"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tauri-apps/cli": "^2.9.4",
    "concurrently": "^9.2.1",
    "vite": "^5.4.21"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-fs": "^2.4.4",
    "@tauri-apps/plugin-shell": "^2.3.3"
  }
}